<?xml version="1.0" ?>
<robot name="Thesis" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- BGN - Include -->
  <xacro:include filename="$(find thesis_description)/urdf/links_joints.xacro" />
  <xacro:include filename="$(find thesis_description)/urdf/gazebo.xacro" />
  <!-- END - Include -->
  
  <material name="white">
    <color rgba="1 1 1 1"/>
